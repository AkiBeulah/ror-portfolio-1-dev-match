<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1 class="text-center">Create your user profile.</h1>
    <p class="text-center">Become a part of the Dev Match community by creating this profile.</p>
    <div class="flash-messages">
      <% flash.each do |key, value| %>
        <%= content_tag :div, value, class: "alert alert-#{key}" %>
      <% end %>
    </div>

    <div class="well">
      <%= render 'form' %>
    </div>
  </div>
</div>